<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             xmlns:vm="clr-namespace:TweetX.ViewModels"
             x:Class="TweetX.Views.Content.TimelineView">

  <ItemsControl Items="{Binding Timeline.StatusCollection}">

    <ItemsControl.ItemTemplate>
      <DataTemplate>
        <TextBlock DataContext="{Binding Mode=OneWay}" Text="{Binding Id, FallbackValue=Test}" />
      </DataTemplate>
    </ItemsControl.ItemTemplate>
  </ItemsControl>
</UserControl>