<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="300" d:DesignHeight="450"
             Opacity="{DynamicResource Dim1}"
             x:Class="Loon.Views.Content.TweetItem.TweetItemRetweetedBy">

  <StackPanel Orientation="Horizontal">
    <TextBlock Text="{DynamicResource RetweetSymbol}" FontFamily="{DynamicResource SymbolFont}" />
    <TextBlock Text="@" Margin="5 0 0 0" />
    <TextBlock Text="{Binding User.ScreenName}" />
    <TextBlock Text="{DynamicResource tweet-retweeted}" Margin="5 0 0 0" />
  </StackPanel>
</UserControl>